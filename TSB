-- Scripts:

local function ENPE_fake_script() -- Main.Lunar 
	local script = Instance.new('LocalScript', Main)

	game.Lighting.ClockTime = 0 -- night
	
	local Players = game:GetService("Players")
	local UIS = game:GetService("UserInputService")
	local Client = Players.LocalPlayer
	local Character = Client.Character
	local Humanoid = Character:WaitForChild("Humanoid")
	local RootPart = Character:WaitForChild("HumanoidRootPart")
	local CAS = game:GetService("ContextActionService")
	local Camera = workspace.CurrentCamera
	
	local Locations = {
		["Void"] = CFrame.new(376.597, 335.452, -60.091),
		["Death Cutscene"] = CFrame.new(-65.857, 36.001, 20382.965),
		["Main Map"] = CFrame.new(124.683, 440.756, -34.319),
		["Atomic Slash Room"] = CFrame.new(1077.489, 141.941, 23021.76)
	}
	
	local Settings = {
		NoStun = false,
		ESP = false,
		NoRagdoll = false,
	}
	
	local RagdollInstances = {
		"Ragdoll",
		"RagdollSim",
	}
	
	local StunInstances = {
		"Freeze",
		"Slowed",
		"NoJump",
		"NoBlock",
		"ComboStun",
		"NoVel",
		"LaunchEnded",
		"StopRunning",
	}
	
	local function ApplyNoStun()
		while task.wait() do
			if (Settings.NoStun == false) then
				break
			end
	
			RootPart.Anchored = false
	
			for index, Value in pairs(Character:GetChildren()) do
				if (table.find(StunInstances, Value.Name)) then
					Value:Destroy()
				end
			end
		end
	end
	
	local function ApplyNoRagdoll()
		while task.wait() do
			if (Settings.NoRagdoll == false) then
				break
			end
	
			for index, Value in pairs(Character:GetChildren()) do
				if (table.find(RagdollInstances, Value.Name)) then
					Value:Destroy()
				end
			end
		end
	end
	
	local function ApplyESP()
		while task.wait() do
			if (Settings.ESP == false) then
				break
			end
	
			for index, Value in pairs(Players:GetPlayers()) do
				if (Value ~= Client) then
					local ESP = Instance.new("Highlight", Value.Character);
	
					ESP.Name = "ESP"
				end
			end
		end
	end
	
	local function UnapplyESP()
		while task.wait() do
			if (Settings.ESP == true) then
				break
			end
			for index, Player in pairs(Players:GetPlayers()) do
				if (Player ~= Client) then
					for _, Value in pairs(Player.Character:GetChildren()) do
						if (Value:IsA("Highlight") and Value.Name == "ESP") then
							Value:Destroy()
						end
					end
				end
			end
		end
	end
	local Main = script.Parent
	local ScreenGui = Main.Parent
	local Title = Main.Title
	local Functions = {
		["Movement"] = Main:FindFirstChild("Movement"),
		["Blatant"] = Main:FindFirstChild("Blatant"),
		["Render"] = Main:FindFirstChild("Render"),
		["Teleports"] = Main:FindFirstChild("Teleports")
	}
	local Tabs = Main.Tabs
	local TabsT = {
		["Movement"] = Tabs:FindFirstChild("Movement"),
		["Blatant"] = Tabs:FindFirstChild("Blatant"),
		["Render"] = Tabs:FindFirstChild("Render"),
		["Teleports"] = Tabs:FindFirstChild("Teleports")
	}
	local TabsToggle = {
		["Movement"] = false,
		["Blatant"] = false,
		["Render"] = false,
		["Teleports"] = false
	}
	local Savedpos = UDim2.new(0.083,0,0.285,0)
	for i,k in pairs(TabsT) do
		k.Activated:Connect(function()
			local TabName = k.Name
			for _,k2 in pairs(Functions) do
				if k2.Name ~= TabName then
					k2.Visible = false
					k2.Position = UDim2.new(10,10,10,10)
				else
					k2.Visible = true
					k2.Position = Savedpos
				end
			end
		end)
	end
	-- Blatant 
	local NoRagdoll = Functions.Blatant["No-Ragdoll"].Text
	local NoStun = Functions.Blatant["No-Stun"].Text
	NoRagdoll.Activated:Connect(function()
		pcall(function()
			if (Settings.NoRagdoll == false) then
				Settings.NoRagdoll = true
				ApplyNoRagdoll()
			else
				Settings.NoRagdoll = false
			end
		end)
	end)
	NoStun.Activated:Connect(function()
		pcall(function()
			if (Settings.NoStun == false) then
				Settings.NoStun = true
				ApplyNoStun()
			else
				Settings.NoStun = false
			end
		end)
	end)
	--Render
	local Red = ColorSequence.new{
		ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(150, 0, 0))
	}
	local Green = ColorSequence.new{
		ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 255, 0)),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 150, 0))
	}
	local ESP = Functions.Render.ESP.Text
	ESP.Activated:Connect(function()
		pcall(function()
			if (Settings.ESP == false) then
				Settings.ESP = true
				ESP.Parent.Toggle.UIGradient.Color = Green
				ApplyESP()
	
			else 
				ESP.Parent.Toggle.UIGradient.Color = Red
				Settings.ESP = false
				UnapplyESP()
			end
		end)
	end)
	-- Teleports
	function VoidTP()
		Client.Character:FindFirstChild("HumanoidRootPart").CFrame = Locations.Void
	end
	function DCTP()
		Client.Character:FindFirstChild("HumanoidRootPart").CFrame = Locations["Death Cutscene"]
	end
	function MapTP()
		Client.Character:FindFirstChild("HumanoidRootPart").CFrame = Locations["Main Map"]
	end
	function ASRTP()
		Client.Character:FindFirstChild("HumanoidRootPart").CFrame = Locations["Atomic Slash Room"]
	end
	local Void = Functions.Teleports.Void.Text
	local DC = Functions.Teleports.DC.Text
	local Map = Functions.Teleports.MainMap.Text
	local ASR = Functions.Teleports.AtomicSR.Text
	local Toggle = false
	local Toggle2 = false
	local Toggle3 = false
	local Toggle4 = false
	
	local ToggleUI = Functions.Teleports.Void.Toggle.UIGradient
	
	
	Void.Activated:Connect(function()
		if Toggle == false then
			Toggle = true
			Void.Parent.Toggle.UIGradient.Color = Green
		else 
			Toggle = false
			Void.Parent.Toggle.UIGradient.Color = Red
		end
	end)
	DC.Activated:Connect(function()
		if Toggle2 == false then
			Toggle2 = true
			DC.Parent.Toggle.UIGradient.Color = Green
		else 
			Toggle2 = false
			DC.Parent.Toggle.UIGradient.Color = Red
		end
	end)
	ASR.Activated:Connect(function()
		if Toggle3 == false then
			Toggle3 = true
			ASR.Parent.Toggle.UIGradient.Color = Green
		else 
			Toggle3 = false
			ASR.Parent.Toggle.UIGradient.Color = Red
		end
	end)
	Map.Activated:Connect(function()
		if Toggle4 == false then
			Toggle4 = true
			Map.Parent.Toggle.UIGradient.Color = Green
		else 
			Toggle4 = false
			Map.Parent.Toggle.UIGradient.Color = Red
		end
	end)
	UIS.InputEnded:Connect(function(input,istyp)
		if istyp then return end
		if input.KeyCode == Enum.KeyCode.Z and Toggle then
			VoidTP()
		elseif input.KeyCode == Enum.KeyCode.X and Toggle2 then
			DCTP()
		elseif input.KeyCode == Enum.KeyCode.C and Toggle3 then
			ASRTP()
		elseif input.KeyCode == Enum.KeyCode.V and Toggle4 then
			MapTP()
		end
	end)
	local Mouse = game.Players.LocalPlayer:GetMouse()
	local WalkSpeed = Functions.Movement.WalkSpeed
	local Trigger = WalkSpeed.Trigger
	local Slider = WalkSpeed.Frame
	local MaxWalkSpeed = 200
	function Update()
		local output =(Mouse.X-WalkSpeed.AbsolutePosition.X)/WalkSpeed.AbsoluteSize.X
	
		if output > 1 then
			output = 1
		elseif output < 0 then
			output = 0
		end
		local WS = math.floor(MaxWalkSpeed * output)
		Trigger.WalkS.Text =  WS.." - WalkSpeed"
		game:GetService("TweenService"):Create(Slider,TweenInfo.new(0.5),{Size = UDim2.fromScale(output,0.9)}):Play()
		pcall(function()
			repeat task.wait()
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (WS)
			until false
		end)
		--	Slider.Size = UDim2.fromScale(output,1)
	end
	local SToggle = false
	function SliderOn()
		SToggle = true
		while true do task.wait()
			if SToggle == false then break end
			Update()
		end
	end
	Trigger.MouseButton1Down:Connect(SliderOn)
	game:GetService("UserInputService").InputEnded:Connect(function(input,istyp)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			SToggle = false
		end
	end)
	local WalkSpeed2 = Functions.Render.FOV
	local Trigger2 = WalkSpeed2.Trigger
	local Slider2 = WalkSpeed2.Frame
	local FOV = 120
	function Update2()
		local output =(Mouse.X-WalkSpeed2.AbsolutePosition.X)/WalkSpeed2.AbsoluteSize.X
		if output > 1 then
			output = 1
		elseif output < 0 then
			output = 0
		end
		local WS = math.floor(FOV * output)
		Trigger2.WalkS.Text =  WS.." - FOV"
		game:GetService("TweenService"):Create(Slider2,TweenInfo.new(0.5),{Size = UDim2.fromScale(output,0.9)}):Play()
		pcall(function()
			repeat task.wait()
				game:GetService("TweenService"):Create(workspace.CurrentCamera,TweenInfo.new(0.5),{FieldOfView = WS}):Play()
			until false
		end)
		--	Slider.Size = UDim2.fromScale(output,1)
	end
	local SToggle2 = false
	function SliderOn2()
		SToggle2 = true
		while true do task.wait()
			if SToggle2 == false then break end
			Update2()
		end
	end
	Trigger2.MouseButton1Down:Connect(SliderOn2)
	game:GetService("UserInputService").InputEnded:Connect(function(input,istyp)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			SToggle2 = false
		end
	end)
end
coroutine.wrap(ENPE_fake_script)()
local function PJHCBG_fake_script() -- Main.Drag 
	local script = Instance.new('LocalScript', Main)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(PJHCBG_fake_script)()
